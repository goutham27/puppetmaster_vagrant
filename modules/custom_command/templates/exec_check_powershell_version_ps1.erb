$expected_version = [int](<%= @expected_version %>)
$version = $host.Version.Major
write-output "DEBUG: expected_version=${expected_version}"
write-output "DEBUG: version=${version}"
$status = ($version -lt $expected_version)
write-output "DEBUG: (${version} -lt ${expected_version} ) => $status"
# converting Powershell statuses to appropriate exitcodes
$exitcode  =  [int]( -not ($status))
write-output "will exit with ${exitcode}"
exit $exitcode
